define(function(require) {

	ko.validation.registerExtenders();

	var modelView = function(data) {
		this.dataModel = {
			srvcPlanName : ko.observable(data.srvcPlanName).extend({
				required : true
			}),
			srvcPlanCreatDate : ko.observable(data.srvcPlanCreatDate),
			srvcPlanRates:ko.observableArray(data.serviceRateSet)
		};
		
		this.viewModel = {
			isIdEditable : ko.observable(false),
			isEditable : ko.observable(false),
			isSelected : ko.observable(false)
		};

	};

	return modelView;

});