<table id="srvcPlanMgrTable" class="table table-bordered gridTable">
	<thead>
		<tr>
			<th>Sl#</th>
			<th>SERVICE PLAN NAME</th>
			<th>SERVICE PLAN CREATION DATE</th>
			<th colspan="3">ACTION</th>
		</tr>
	</thead>
	<tbody data-bind="foreach:srvcPlanPagedGridView().pagedRecordList">
		<tr data-toggle="collapse"
			data-bind="attr:{'data-target':'#'+$index()}"
			class="accordion-toggle">
			<td data-bind="text:$index()+1"><span class="icon-edit"></span>
			</td>
			<td><input
				data-bind='value:dataModel.srvcPlanName,hasFocus:viewModel.isSelected,enable:viewModel.isIdEditable'></input></td>
